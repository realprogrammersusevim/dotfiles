{
  "Blog Entry": {
    "prefix": "blog",
    "body": [
      "---",
      "id: ${1:id}",
      "title: ${2:title}",
      "date: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
      "tags: ${3:tags}",
      "draft: true",
      "---"
    ]
  }, 
  "Link Entry": {
    "prefix": "link",
    "body": [
      "---",
      "id: ${1:id}",
      "title: ${2:title}",
      "link: ${3:link}",
      "via: ${4:via}",
      "date: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
      "tags: ${5:tags}",
      "draft: true",
      "---"
    ]
  }
}
